        <div id="manageUser" class="tabcontent">

            <div class="hero-image">
            </div>

            <div class="card" id="profilePageAboutMeCard" style="width: 100%; margin-top: 0;">
                <div>
                    <hr>
                    <h6>Search User by Email</h6>
                    <nav>
                        <div class="nav-wrapper">
                            <form id="userSearch">
                                <div class="input-field" id="searchblock">
                                    <input id="searchforUser" type="search" required>
                                    <label class="label-icon" for="search"><i class="material-icons">search</i></label>
                                    <i class="material-icons">close</i>
                                </div>
                            </form>
                            <h3 id="email-err-msg"></h3>
                        </div>
                    </nav>
                    <hr>
                    <h3 style="font-size: 1.7vw;">User Account Information</h3>
                    <hr>
                    <form class="signup col s12 m12 l12">
                        <div class="row">
                            <div class="input-field col s2">
                                <input disabled placeholder="AccountId" id="accountid" type="text"
                                    value="{{searchedUser.id}}">
                                <label for="id">Account ID</label>
                            </div>
                            <div class="welcomMessage col s10">
                                <h2>{{searchedUser.first_name}} {{searchedUser.last_name}}</h2>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <input placeholder="First Name" id="firstnameinput" type="text"
                                    value="{{searchedUser.first_name}}">
                                <label for="first_name">First Name*</label>
                            </div>
                            <div class="input-field col s6">
                                <input placeholder="Last Name" id="lastnameinput" type="text"
                                    value="{{searchedUser.last_name}}">
                                <label for="last_name">Last Name*</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <input placeholder="Address1" id="addressinput" type="text"
                                    value="{{searchedUser.address}}">
                                <label for="address">Address1*</label>
                            </div>
                            <div class="input-field col s6">
                                <input placeholder="Address2" id="address2input" type="text"
                                    value="{{searchedUser.address2}}">
                                <label for="address2">Address2</label>
                            </div>
                        </div>

                        <div class="row">
                            <div class="input-field col s6">
                                <input placeholder="City" id="cityinput" type="text" value="{{searchedUser.city}}">
                                <label for="city">City*</label>
                            </div>

                            <div class="input-field col s6">
                                <input placeholder="State" id="stateinput" type="text" value="{{searchedUser.state}}">
                                <label for="state">State*</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <input placeholder="Zip Code" id="zipcodeinput" type="text"
                                    value="{{searchedUser.zip}}">
                                <label for="zipCode">Zip Code*</label>
                            </div>

                            <div class="input-field col s6">
                                <input placeholder="Phone Number" id="phoneinput" type="text"
                                    value="{{searchedUser.phone}}">
                                <label for="phoneNumber">Phone Number*</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <input placeholder="EmailId" id="emailinput1" type="text" value="{{searchedUser.email}}">
                                <label for="email">EmailId*</label>
                            </div>
                        </div>

                        <hr>
                        <h3 style="font-size: 1.7vw;">School Afilliation</h3>
                        <div class="row">
                            <div class="input-field col s12">
                                <input disabled placeholder="School" id="schoolinput" type="text"
                                    value="{{searchedUser.school}}">
                                <label for="school">School*</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <input placeholder="Role" id="roleinput" type="text" value="{{searchedUser.role_name}}">
                                <label for="role">Role*</label>
                            </div>
                        </div>
                        <button type="submit" class="btn btn-default red darken-4" id="updateUserButton">UPDATE
                            USER</button>
                        <button type="submit" data-target="delete-account-modal"
                            class="btn btn-default red darken-4 modal-trigger" id="deleteButton">DELETE USER</button>

                        <hr>
                    </form>

                </div>
            </div>
            <div id="delete-account-modal" class="modal">
                <div class="modal-content">
                    <h4>Confirmation</h4>
                    <div class="modal-header">
                        <h6> Are you sure you want to delete this account?</h6>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-default red darken-4" id="confirm-delete">YES</button>
                        <button type="submit" class="btn btn-default green darken-4" id="cancel-delete">CANCEL</button>
                    </div>

                </div>

            </div>
        </div>